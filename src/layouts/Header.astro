---
import Logo from '../svg/Logo.astro'
---
<header class="animate__animated animate__fadeInDown" >
    <Logo></Logo>
    <div class="header-footer-divider"></div>
</header>

<style>
    header{
        height: 65px;
        justify-content: stretch;
        align-items: stretch;
        flex-direction: column;
        display: flex;
    }
    img {
        width: 100%;
        object-fit: cover;
        height: 200px;
    }
</style>